{
  "plugins": ["prettier-plugin-tailwindcss"],
  "arrowSpacing": ["error", { "before": true, "after": true }],
  "singleQuote": true,
  "semi": false,
  "useTabs": false,
  "tabWidth": 2,
  "trailingComma": "none",
  "printWidth": 200,
  "bracketSpacing": true,
  "arrowParens": "always",
  "endOfLine": "auto"
}
